{
    "manifest_version": 2,
    "name": "Tab Capture",
    "version": "0.0.1",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "logo.png"
        }
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "web_accessible_resources": [
        "storage.html",
        "storage.js"
    ]
}